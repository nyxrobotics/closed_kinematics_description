<?xml version="1.0" encoding="UTF-8"?>

<launch> 
	<param name="robot_description_sdf" textfile="$(find closed_kinematics_description)/sdf/simple_open_chain.sdf" />
	<param name="robot_description"     textfile="$(find closed_kinematics_description)/urdf/simple_open_chain.urdf" />
	<!-- <param name="robot_description" textfile="$(find closed_kinematics_description)/sdf/simple_open_chain.sdf" /> -->
	<node
	  name="spawn_model"
	  pkg="gazebo_ros"
	  type="spawn_model"
	  args="-sdf -param robot_description_sdf -model closed_chain -x 0 -y 0.6 -z 0.3"
	  output="screen">
	</node>
<!-- 	<node
	  name="spawn_model"
	  pkg="gazebo_ros"
	  type="spawn_model"
	  args="-urdf -param robot_description -model closed_chain -x 0 -y 0.6 -z 0.3"
	  output="screen">
	</node> -->
	<rosparam file="$(find closed_kinematics_description)/config/open_chain.yaml" command="load"/> 
	<node name="controller_spawner" 
	    pkg="controller_manager" 
	    type="spawner" respawn="false" output="screen" ns="/closed_chain"
	    args="joint1_controller joint_state_controller"/>

