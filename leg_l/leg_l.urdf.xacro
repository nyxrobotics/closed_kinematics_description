			<inertial>
				<mass value="0.01646643"/>
				<origin xyz="0.05000000 0.02495100 -0.00491150" rpy="0 0 0"/>
				<inertia
					ixx="0.00000640"
					ixy="0.00000000" iyy="0.00001642"
					ixz="0.00000000" iyz="0.00000000" izz="0.00002264"/>
			</inertial>
		</link>
		<joint name="${name}_joint5" type="revolute">
	    	<parent link="${name}_link3"/>
	    	<child  link="${name}_link5"/>
	    	<origin xyz="-0.0213 0 -0.02655" rpy="0 2.3562 0"/>
	    	<axis xyz="0 -1 0"/>
	    	<limit effort="1000.0" lower="-3.141593" upper="3.141593" velocity="0.5"/>
	    	<mimic joint="${name}_joint4" multiplier="1" offset="0"/> 
	  	</joint>



		<!-- toe -->
		<link name="${name}_link6">
	    	<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>                                                              
					<mesh filename="package://ginko_description/meshes/stl/link06_l_toe01.stl"/>
