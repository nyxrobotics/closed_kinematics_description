			<limit effort="1000.0" lower="-0.7854" upper="1.3" velocity="0.5"/>
		</joint>

		<!-- parallel link (lower-2nd) -->
		<link name="${name}_link5">
	    	<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
