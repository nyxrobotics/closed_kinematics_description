<?xml version="1.0"?>


<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
	<xacro:macro name="leg_l" params="name parent">
	<!-- <xacro:property name="name" value="leg"/> -->

		<!-- Link conencts from body. Femoral first. -->
		<link name="${name}_link0">
			<!-- Collision created from STL mesh without color -->
			<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>                                                              
					<mesh filename="package://ginko_description/meshes/stl/link02_l_femoral.stl"/>
				</geometry>
			</collision>
			<!-- Visualisation created from Color dae -->
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://ginko_description/meshes/stl/link02_l_femoral.stl"/>
					<!-- <mesh filename="package://ginko_description/meshes/dae/link02_l_femoral.dae"/> -->
				</geometry>
			</visual>
			<inertial>
				<mass value="0.03433352"/>
				<origin xyz="0.02930795 0.03723759 -0.02605386" rpy="0 0 0"/>
				<inertia
					ixx="0.00002443"
					ixy="-0.00000021" iyy="0.00002287"
					ixz="-0.00000021" iyz="-0.00000391" izz="0.00002983"/>
			</inertial>
		</link>
<!-- 		<joint name="${name}_joint0" type="revolute">
		    <parent link="${parent}"/>
			<child link="${name}_link0"/>
			<origin xyz="-0.0404 0.0 -0.0409" rpy="0 0 0"/>
		    <axis xyz="-1 0 0"/>
			<limit effort="1000.0" lower="-0.8" upper="1.1781" velocity="0.5"/>
		</joint> -->


		<!-- parallel link (upper-1st) -->
		<link name="${name}_link1">
			<!-- Collision created from STL mesh without color -->
			<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>                                                              
					<mesh filename="package://ginko_description/meshes/stl/link03_l_leg01.stl"/>
	    		</geometry>
			</collision>
			<!-- Visualisation created from Color dae -->
	    	<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>
					<mesh filename="package://ginko_description/meshes/stl/link03_l_leg01.stl"/>
					<!-- <mesh filename="package://ginko_description/meshes/dae/link03_l_leg01.dae"/> -->
	    		</geometry>
			</visual>
			<inertial>
				<mass value="0.18525692"/>
				<origin xyz="0.05000000 0.03285059 0.00068146" rpy="0 0 0"/>
				<inertia
					ixx="0.00041331"
					ixy="-0.00003605" iyy="0.00027917"
					ixz="-0.00000000" iyz="-0.00000184" izz="0.00066510"/>
			</inertial>
		</link>
		<joint name="${name}_joint1" type="revolute">
			<parent link="${name}_link0"/>
			<child  link="${name}_link1"/>
			<origin xyz="0.0433 0.0175 -0.0481" rpy="0 0.7854 0"/>
			<axis xyz="0 -1 0"/>
			<limit effort="1000.0" lower="-1.3" upper="0.7854" velocity="0.5"/>
		</joint>

		<!-- parallel link (upper-2nd) -->
		<link name="${name}_link2">
			<!-- Collision created from STL mesh without color -->
	    	<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>                                                              
					<mesh filename="package://ginko_description/meshes/stl/link04_l_leg02.stl"/>
	    		</geometry>
			</collision>
			<!-- Visualisation created from Color dae -->
