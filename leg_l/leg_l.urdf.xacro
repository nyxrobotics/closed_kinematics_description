			</visual>
			<inertial>
				<mass value="0.01646643"/>
				<origin xyz="0.05000000 0.02495100 -0.00491150" rpy="0 0 0"/>
				<inertia
					ixx="0.00000640"
					ixy="0.00000000" iyy="0.00001642"
					ixz="0.00000000" iyz="0.00000000" izz="0.00002264"/>
			</inertial>
		</link>
		<joint name="${name}_joint2" type="revolute">
	    	<parent link="${name}_link0"/>
	    	<child  link="${name}_link2"/>
	    	<origin xyz="0.022 0.0175 -0.06015" rpy="0 0.7854 0"/>
	    	<axis xyz="0 -1 0"/>
	    	<limit effort="1000.0" lower="-3.141593" upper="3.141593" velocity="0.5"/>
	    	<!-- <mimic joint="${name}_joint1" multiplier="1" offset="0"/>  -->
	  	</joint>


		<!-- knee -->
		<link name="${name}_link3">
	    	<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>
					<mesh filename="package://ginko_description/meshes/stl/link05_l_knee.stl"/>
	    		</geometry>
			</collision>
	    	<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>
					<mesh filename="package://ginko_description/meshes/stl/link05_l_knee.stl"/>
					<!-- <mesh filename="package://ginko_description/meshes/dae/link05_l_knee.dae"/> -->
	    		</geometry>
			</visual>
			<inertial>
				<mass value="0.01781176"/>
				<origin xyz="0.00393357 0.02364163 -0.01930000" rpy="0 0 0"/>
				<inertia
					ixx="0.00001301"
					ixy="0.00000008" iyy="0.00000700"
					ixz="0.00000000" iyz="0.00000000" izz="0.00001388"/>
			</inertial>
		</link>
		<joint name="${name}_joint3" type="revolute">
	    	<parent link="${name}_link1"/>
	    	<child  link="${name}_link3"/>
	    	<origin xyz="0.1 0.0 0.0" rpy="0 -0.7854 0"/>
	    	<axis xyz="0 1 0"/>
	    	<limit effort="1000.0" lower="-3.141593" upper="3.141593" velocity="0.5"/>
	  	</joint>


