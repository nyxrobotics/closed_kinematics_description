		<joint name="${name}_joint6" type="revolute">
	    	<parent link="${name}_link4"/>
	    	<child  link="${name}_link6"/>
	    	<origin xyz="0.1 0.0 0.0" rpy="0 -2.3562 0"/>
	    	<axis xyz="0 1 0"/>
	    	<limit effort="1000.0" lower="-3.141593" upper="3.141593" velocity="0.5"/>
	  	</joint>

		<link name="${name}_link7">
	    	<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>                                                              
					<mesh filename="package://ginko_description/meshes/stl/link07_l_toe02.stl"/>
